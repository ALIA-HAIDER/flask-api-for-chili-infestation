# save as check_mem.py and run: python check_mem.py
import psutil, os, torch
proc = psutil.Process()
print(RSS before:, proc.memory_info().rss)   # bytes
m = torch.load(model.pth, map_location=cpu)
print(RSS after load:, proc.memory_info().rss)
# optional: delete model to free
del m
print(RSS after del:, proc.memory_info().rss)


